{
  "name": "dropdown-extension",
  "version": "1.0",
  "description": "An extension to drop img files from chrome to ios devices",
  "permissions": ["storage", "contextMenus"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
      "default_popup": "popup.html"
  },
  "manifest_version": 2
}
